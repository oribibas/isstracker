{
    "name": "ISS Distance",
    "version": "1.0",
    "description": "Show your current locations distance from the International Space Station",
    "manifest_version": 2,
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icons/international-space-station-icon.png"
    },
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js; object-src 'self'",
    "permissions": [
      "geolocation"
    ]
}
